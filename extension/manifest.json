{
  "name": "Most Important Thing",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Don't let the browser stop you from getting things done.",
  "homepage_url": "http://google.com",
  "icons": {
    "128": "icons/clock.png"
  },
  "background": {
    "page": "html/background.html",
    "persistent": true
  },
  "options_page": "html/options.html",
  "browser_action": {
    "default_title": "Most Important Thing",
    "default_popup": "html/browserAction.html",
    "default_icon": {
      "19": "icons/clock.png",
      "38": "icons/clock.png"
    }
  },
  "permissions": [
    "identity",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "key": ""
}